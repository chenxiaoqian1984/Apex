    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Stock_Code_is_number</fullName>
        <active>true</active>
        <description>股票代码必须是由6位数字构成</description>
        <errorConditionFormula>!(ISNUMBER(Stock_Code__c) &amp;&amp; LEN(Stock_Code__c)  = 6)</errorConditionFormula>
        <errorDisplayField>Stock_Code__c</errorDisplayField>
        <errorMessage>股票编码必须是6位数字</errorMessage>
    </validationRules>
